                                            ToolTip="{x:Static localization:Strings.Clear}"
                                        >
                                            <Button.Style>
                                                <Style TargetType="Button" BasedOn="{StaticResource ImageButton}">
                                                    <Setter Property="IsEnabled" Value="True"/>
                                                    <Style.Triggers>
