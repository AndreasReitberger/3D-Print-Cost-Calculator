                                                        <templates:FileViewInfoTemplate.ContextMenu>
                                                            <ContextMenu>
                                                                <MenuItem Header="delete"/>
                                                                <MenuItem Header="Item 2"/>
                                                            </ContextMenu>
                                                        </templates:FileViewInfoTemplate.ContextMenu>
                                                        -->
                                                        </templates:FileViewInfoTemplate>
                                                    </DataTemplate>
