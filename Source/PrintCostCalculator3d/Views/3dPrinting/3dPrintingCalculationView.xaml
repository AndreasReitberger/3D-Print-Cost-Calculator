                                                        <Style.Triggers>
                                                            <MultiDataTrigger>
                                                                <MultiDataTrigger.Conditions>
                                                                    <Condition Binding="{Binding ElementName=mslbGCode, Path=SelectedItems.Count}" Value="0" />
                                                                </MultiDataTrigger.Conditions>
