                                                </h:HelixViewport3D>
                                                -->
                                            </DockPanel>
                                        </DockPanel>
                                    </TabItem>
                                </TabControl>
                            </DockPanel>
                        </Grid>
                    </TabItem>

                </TabControl>
            </Grid>
        </Expander>

        
    </Grid>
