                                                    <Rectangle.OpacityMask>
                                                        <VisualBrush Stretch="Uniform" Visual="{iconPacks:Material Kind=NotificationClearAll}" />
                                                    </Rectangle.OpacityMask>
                                                </Rectangle>

                                            </Grid>
                                        </Button>

