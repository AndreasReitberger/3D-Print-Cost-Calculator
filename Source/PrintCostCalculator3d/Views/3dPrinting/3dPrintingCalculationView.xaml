                                                    <hdx:DirectionalLight3D Direction="{Binding GcodeCamera.LookDirection}" Color="{DynamicResource MahApps.Colors.Accent}" />
                                                    <hdx:GroupModel3D 
                                                        x:Name="group" 
                                                        ItemsSource="{Binding Gcode3dLayerGeometry}" 
                                                        
                                                        />
                                                </hdx:Viewport3DX>

